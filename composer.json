{
    "name": "resgen/lumen-proc",
    "require": {
        "php" : ">=7.3",
        "laravel/lumen-framework" : ">=5.8"
    },
    "require-dev": {
        "phpunit/phpunit" : "^7.0",
        "mockery/mockery" : "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Resgen\\Common\\Proc\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}